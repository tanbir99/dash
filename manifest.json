{
    "name": "dash!",
    "version": "0.0.1",
    "description": "School planner and class reminder.",
    "background": {
      "scripts": ["/js/background.js"],
      "persistent": true
    },
    "icons": {
      "16": "/images/LogoMakr_3aljcG.png",
      "24": "/images/LogoMakr_3aljcG.png",
      "32": "/images/LogoMakr_3aljcG.png",
      "128": "/images/LogoMakr_3aljcG.png"
    },
    "browser_action": {
      "default_icon": "/images/LogoMakr_3aljcG.png",
      "default_popup": "/html/popup.html",
      "default_title": "dash!"
    },
    "options_page": "/html/options.html",

    "permissions": [
      "storage"
    ],

    "content_security_policy": "script-src 'self' https://ajax.googleapis.com/; object-src 'self'",

    "manifest_version": 2
  }